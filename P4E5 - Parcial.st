Object subclass: #Empleado	instanceVariableNames: ''	classVariableNames: ''	package: 'P4E5-Model'!!Empleado methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 02:12'!basico	SubclassResponsibility ! !!Empleado methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 02:13'!adicional	SubclassResponsibility ! !!Empleado methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:57'!sueldo	^(self sueldoBasico + self aportesPatronales)! !!Empleado methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 02:17'!descuento	^ (self basico * 0.13) + (self adicional * 0.05)! !!Empleado methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:55'!aportesPatronales	|porcentaje|	porcentaje := 0.01. 	(self sueldoBasico > 4000)		ifTrue: [ porcentaje := 0.03 ]		ifFalse: [ (self sueldoBasico > 3000) 			ifTrue: [porcentaje := 0.02]].	^(self sueldoBasico * porcentaje)! !!Empleado methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:54'!sueldoBasico	^(self basico + self adicional - self descuento)! !Empleado subclass: #Contratado	instanceVariableNames: 'horas'	classVariableNames: ''	package: 'P4E5-Model'!!Contratado methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:14'!horas: unNumero	horas := unNumero! !!Contratado methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:12'!basico 	^self horas * 40! !!Contratado methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:18'!adicional	|adicion|	adicion := 0.	(horas >= 100)		ifTrue: [ adicion := 200 ]		ifFalse: [ (horas > 50)	ifTrue: [adicion := 100] ].	^adicion.! !!Contratado methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:13'!horas	^horas! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Contratado class	instanceVariableNames: ''!!Contratado class methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:35'!trabajo: unNumero	|nuevoEmpleado|	nuevoEmpleado := self new.	nuevoEmpleado horas: unNumero.	^nuevoEmpleado! !Empleado subclass: #Pasante	instanceVariableNames: 'examenes'	classVariableNames: ''	package: 'P4E5-Model'!!Pasante methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 02:54'!basico	^2000! !!Pasante methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:17'!adicionalPorExamenes	^100 * examenes! !!Pasante methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:05'!adicional	^ self adicionalPorExamenes ! !!Pasante methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:09'!examenes: unNumero	examenes := unNumero! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pasante class	instanceVariableNames: ''!!Pasante class methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:14'!conExamenesAprobados: unNumero	|pasante|	pasante := self new.	pasante examenes: unNumero.	^pasante! !Empleado subclass: #Profesional	instanceVariableNames: 'hijos esCasado'	classVariableNames: ''	package: 'P4E5-Model'!!Profesional methodsFor: 'accessing' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:09'!hijos: unNumero	hijos := unNumero! !!Profesional methodsFor: 'accessing' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:10'!esCasado: unBooleano	esCasado := unBooleano! !!Profesional methodsFor: 'accessing' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:08'!esCasado	^esCasado! !!Profesional methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 02:22'!basico	SubclassResponsibility ! !!Profesional methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:18'!adicionalPorHijos	^100 * hijos! !!Profesional methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:18'!adicional	^ self adicionalPorCasado + self adicionalPorHijos ! !!Profesional methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:17'!adicionalPorCasado	(self esCasado)	ifTrue: [ ^500 ]	ifFalse: [ ^0 ].! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Profesional class	instanceVariableNames: ''!!Profesional class methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:17'!esCasado: unBooleano conHijos: unNumero	|profesional|	profesional := self new.	profesional esCasado: unBooleano.	profesional hijos: unNumero.	^profesional.! !Profesional subclass: #Planta	instanceVariableNames: 'fechaIngreso'	classVariableNames: ''	package: 'P4E5-Model'!!Planta methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 02:50'!basico	^3000! !!Planta methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:19'!adicional	^super adicional + self adicionalPorAntiguedad ! !!Planta methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:45'!antiguedad	|anios|	anios := 0.	(Date today daysLeftInYear < fechaIngreso daysLeftInYear)		ifTrue: [ anios := (Date today) year - (fechaIngreso year) ]		ifFalse:  [ ((Date today) year > fechaIngreso year) 			ifTrue: [ anios := (Date today) year - fechaIngreso year - 1]		].	^anios.! !!Planta methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:07'!adicionalPorAntiguedad	^100 * self antiguedad ! !!Planta methodsFor: 'accessing' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:20'!fechaIngreso: unaFecha	fechaIngreso := unaFecha ! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Planta class	instanceVariableNames: ''!!Planta class methodsFor: 'accessing' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:34'!ingresoEl: unaFecha esCasado: unBooleano conHijos: unNumero 	|planta|	planta := self esCasado: unBooleano conHijos: unNumero.	planta fechaIngreso: unaFecha. 	^planta! !Profesional subclass: #Temporario	instanceVariableNames: 'horasTrabajadas'	classVariableNames: ''	package: 'P4E5-Model'!!Temporario methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:21'!basico	^4000 + self basicoDeHoras ! !!Temporario methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:21'!basicoDeHoras	^10 * horasTrabajadas ! !!Temporario methodsFor: 'accessing' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:12'!horasTrabajadas: unNumero	horasTrabajadas := unNumero! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Temporario class	instanceVariableNames: ''!!Temporario class methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:24'!esCasado: unBooleano conHijos: unNumero yTrabajo: unaCantidadDeHoras	|temporario|	temporario := self esCasado: unBooleano conHijos: unNumero.	temporario horasTrabajadas: unaCantidadDeHoras.	^temporario ! !TestCase subclass: #EmpleadoTest	instanceVariableNames: 'pasante temporario planta'	classVariableNames: ''	package: 'P4E5-Tests'!!EmpleadoTest methodsFor: 'running' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:43'!setUp 	|fechaIngreso|	fechaIngreso := '2018-06-05' asDateAndTime.	pasante := Pasante conExamenesAprobados: 8.	planta := Planta ingresoEl: fechaIngreso esCasado: true conHijos: 2.	temporario := Temporario esCasado: false conHijos: 0 yTrabajo: 100! !!EmpleadoTest methodsFor: 'tests' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:55'!testSueldoBasico	self assert: planta sueldoBasico equals: 3465.	self assert: temporario sueldoBasico equals: 4350.	self assert: pasante sueldoBasico equals: 2500.! !TestCase subclass: #PasanteTest	instanceVariableNames: 'pasante'	classVariableNames: ''	package: 'P4E5-Tests'!!PasanteTest methodsFor: 'tests' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:49'!testAdicional	self assert: pasante adicional equals: 800! !!PasanteTest methodsFor: 'running' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:47'!setUp	pasante := Pasante conExamenesAprobados: 8.! !TestCase subclass: #PlantaTest	instanceVariableNames: 'planta'	classVariableNames: ''	package: 'P4E5-Tests'!!PlantaTest methodsFor: 'running' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:50'!setUp	|fechaIngreso|	fechaIngreso := '2018-06-05' asDateAndTime.	planta := Planta ingresoEl: fechaIngreso esCasado: true conHijos: 2.! !!PlantaTest methodsFor: 'tests' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:50'!testAdicional	self assert: planta adicional equals: 900! !TestCase subclass: #TemporarioTest	instanceVariableNames: 'temporario'	classVariableNames: ''	package: 'P4E5-Tests'!!TemporarioTest methodsFor: 'tests' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:52'!testAdicional	self assert: temporario adicional equals: 0! !!TemporarioTest methodsFor: 'tests' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:52'!testBasico	self assert: temporario basico equals: 5000! !!TemporarioTest methodsFor: 'running' stamp: 'MariaNoelAdrogueBenas 6/12/2020 17:51'!setUp	temporario := Temporario esCasado: false conHijos: 0 yTrabajo: 100! !