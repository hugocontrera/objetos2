nclassified' stamp: 'hd 6/26/2020 17:34'!esArchivo	^ok! !!LinkSimbolico methodsFor: 'as yet unclassified' stamp: 'hd 6/26/2020 17:29'!llamado: linkName apuntadoA: aFileSystemElement! !!LinkSimbolico methodsFor: 'as yet unclassified' stamp: 'hd 6/26/2020 17:31'!esArchivo: unBooleano	ok := unBooleano! !!LinkSimbolico methodsFor: 'as yet unclassified' stamp: 'hd 6/26/2020 17:40'!crearUnLink: unElemento	self esArchivo		ifTrue: [ linksimbolicos add: unElemento.			tamano := 1 ]		ifFalse: [ self error: 'Mensaje de error' ]! !