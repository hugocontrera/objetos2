Object subclass: #Empleado	instanceVariableNames: ''	classVariableNames: ''	package: 'P4E5-Model'!!Empleado methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 02:13'!sueldo	^(self basico + self adicional - self descuento)! !!Empleado methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 02:12'!basico	SubclassResponsibility ! !!Empleado methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 02:17'!descuento	^ (self basico * 0.13) + (self adicional * 0.05)! !!Empleado methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 02:13'!adicional	SubclassResponsibility ! !Empleado subclass: #Pasante	instanceVariableNames: 'examenes'	classVariableNames: ''	package: 'P4E5-Model'!!Pasante methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 02:54'!basico	^2000! !!Pasante methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:17'!adicionalPorExamenes	^100 * examenes! !!Pasante methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:11'!adicional	^(self porExamen * examenes)! !!Pasante methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:09'!examenes: unNumero	examenes := unNumero! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pasante class	instanceVariableNames: ''!!Pasante class methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:14'!conExamenesAprobados: unNumero	|pasante|	pasante := self new.	pasante examenes: unNumero.	^pasante! !Empleado subclass: #Profesional	instanceVariableNames: 'hijos esCasado'	classVariableNames: ''	package: 'P4E5-Model'!!Profesional methodsFor: 'accessing' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:09'!hijos: unNumero	hijos := unNumero! !!Profesional methodsFor: 'accessing' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:10'!esCasado: unBooleano	esCasado := unBooleano! !!Profesional methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 02:22'!basico	SubclassResponsibility ! !!Profesional methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:18'!adicionalPorHijos	^100 * hijos! !!Profesional methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:18'!adicional	^ self adicionalPorCasado + self adicionalPorHijos ! !!Profesional methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:17'!adicionalPorCasado	(self esCasado)	ifTrue: [ ^500 ]	ifFalse: [ ^0 ].! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Profesional class	instanceVariableNames: ''!!Profesional class methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:17'!esCasado: unBooleano conHijos: unNumero	|profesional|	profesional := self new.	profesional esCasado: unBooleano.	profesional hijos: unNumero.	^profesional.! !Profesional subclass: #Planta	instanceVariableNames: 'fechaIngreso'	classVariableNames: ''	package: 'P4E5-Model'!!Planta methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 02:50'!basico	^3000! !!Planta methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:19'!adicional	^super adicional + self adicionalPorAntiguedad ! !!Planta methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:07'!antiguedad	|anios|	anios := 0,	(Date today daysLeftInYear < fechaIngreso daysLeftInYear)		ifTrue: [ anios := Date today year - fechaIngreso today year ]		ifFalse:  [ (Date today year > fechaIngreso year) 			ifTrue: [ anios := Date today year - fechaIngreso today year - 1]		].	^anios.! !!Planta methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:07'!adicionalPorAntiguedad	^100 * self antiguedad ! !!Planta methodsFor: 'accessing' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:20'!fechaIngreso: unaFecha	fechaIngreso := unaFecha ! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Planta class	instanceVariableNames: ''!!Planta class methodsFor: 'accessing' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:21'!ingreso: unaFecha esCasado: unBooleano conHijos: unNumero 	|planta|	planta := self esCasado: unBooleano conHijos: unNumero.	planta fechaIngreso: unaFecha. 	^planta! !Profesional subclass: #Temporario	instanceVariableNames: 'horasTrabajadas'	classVariableNames: ''	package: 'P4E5-Model'!!Temporario methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:21'!basico	^4000 + self basicoDeHoras ! !!Temporario methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 03:21'!basicoDeHoras	^10 * horasTrabajadas ! !!Temporario methodsFor: 'accessing' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:12'!horasTrabajadas: unNumero	horasTrabajadas := unNumero! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Temporario class	instanceVariableNames: ''!!Temporario class methodsFor: 'as yet unclassified' stamp: 'MariaNoelAdrogueBenas 6/12/2020 04:24'!esCasado: unBooleano conHijos: unNumero yTrabajo: unaCantidadDeHoras	|temporario|	temporario := self esCasado: unBooleano conHijos: unNumero.	temporario horasTrabajadas: unaCantidadDeHoras.	^temporario ! !TestCase subclass: #EmpleadoTest	instanceVariableNames: ''	classVariableNames: ''	package: 'P4E5-Tests'!